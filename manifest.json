{
  "manifest_version": 2,
  "name": "Finj",
  "version": "0.1",
  "description": "Font Injector",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/icon.png",
      "48": "assets/icon48.png"
    }
  }
}
